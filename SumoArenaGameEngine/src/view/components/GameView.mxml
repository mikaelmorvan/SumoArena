<?xml version="1.0" encoding="utf-8"?>
<s:SkinnableDataContainer xmlns:fx="http://ns.adobe.com/mxml/2009" 
						  xmlns:s="library://ns.adobe.com/flex/spark" 
						  xmlns:mx="library://ns.adobe.com/flex/mx" 
						  xmlns:vo="model.vo.*"
						  width="{arena.width}" 
						  height="{arena.height}"
						  skinClass="view.components.ArenaSkin"
						  itemRendererFunction="createSphereRenderer">
	<fx:Declarations>
		<vo:Arena id="arena"/>
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			private function createSphereRenderer(item:*):IFactory
			{
				return new ClassFactory(SphereRenderer);
			}
		]]>
	</fx:Script>
</s:SkinnableDataContainer>
